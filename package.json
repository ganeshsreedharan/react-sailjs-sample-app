    "sails-hook-sockets": "^1.5.5",
    "sails-mongo": "^1.0.1",
    "socket.io": "^2.1.1",
    "socket.io-client": "^2.1.1",
    "twitter": "^1.7.1",
    "url-loader": "^1.1.2"
  },
  "devDependencies": {
    "@sailshq/eslint": "^4.19.3",
    "babel-cli": "^6.18.0",
    "babel-core": "^6.18.2",
    "babel-eslint": "^7.1.0",
